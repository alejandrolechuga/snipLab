{
  "name": "http-mocky",
  "version": "1.12.0",
  "description": "Chrome extension to override fetch/XHR responses",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lxieyang/chrome-extension-boilerplate-react.git"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run lint:fix",
      "npm run prettier"
    ]
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build:prod": "ts-node --project tsconfig.scripts.json utils/build.ts --production",
    "build:dev": "ts-node --project tsconfig.scripts.json utils/build.ts --dev",
    "start": "ts-node --project tsconfig.scripts.json utils/webserver.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:silent": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --quiet",
    "lint:commits": "commitlint --edit",
    "prepare": "husky",
    "check-changeset": "ts-node --project tsconfig.scripts.json scripts/check-changeset.ts",
    "ci-check-changeset": "ts-node --project tsconfig.scripts.json scripts/ci-check-changeset.ts",
    "changeset": "npx changeset"
  },
  "dependencies": {
    "@reduxjs/toolkit": "*",
    "class-variance-authority": "*",
    "clsx": "*",
    "lucide-react": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "*",
    "tailwind-merge": "*",
    "tw-animate-css": "*",
    "uuid": "^9.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.27.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/chrome": "^0.0.202",
    "@types/copy-webpack-plugin": "^10.1.3",
    "@types/fs-extra": "^11.0.4",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/testing-library__react": "^10.2.0",
    "@types/uuid": "^10.0.0",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "fs-extra": "^11.1.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-tsc": "^1.6.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^16.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.5.3",
    "react-refresh": "^0.14.0",
    "react-refresh-typescript": "^2.0.7",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "type-fest": "^3.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "zip-webpack-plugin": "^4.0.1"
  }
}
